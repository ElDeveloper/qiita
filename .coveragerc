# this file is based on the examples provided on scikit-learn's .coveragerc

[run]
omit =
    */test*
    */__init__.py
source = qiita_core,qiita_ware,qiita_db,qiita_pet 
branch = True
include = */qiita_*/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
omit =
    */tests*
    */__init__.py
